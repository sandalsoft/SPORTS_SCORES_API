#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/smartwrap@2.0.1/node_modules/smartwrap/src/terminal-adapter.js/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/smartwrap@2.0.1/node_modules/smartwrap/src/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/smartwrap@2.0.1/node_modules/smartwrap/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/smartwrap@2.0.1/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules:/Users/eric/Documents/Development/code/sports-betting/node_modules:/Users/eric/Documents/Development/code/node_modules:/Users/eric/Documents/Development/node_modules:/Users/eric/Documents/node_modules:/Users/eric/node_modules:/Users/node_modules:/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/node_modules/smartwrap/src/terminal-adapter.js/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/node_modules/smartwrap/src/node_modules:/Users/eric/Documents/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/node_modules/smartwrap/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../smartwrap/src/terminal-adapter.js" "$@"
else 
  exec node  "$basedir/../smartwrap/src/terminal-adapter.js" "$@"
fi
