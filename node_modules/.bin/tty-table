#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/tty-table@4.1.3/node_modules/tty-table/adapters/node_modules:/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/tty-table@4.1.3/node_modules/tty-table/node_modules:/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/tty-table@4.1.3/node_modules:/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/.pnpm/node_modules:/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules:/Users/eric/Development/code/sports-betting/node_modules:/Users/eric/Development/code/node_modules:/Users/eric/Development/node_modules:/Users/eric/node_modules:/Users/node_modules:/node_modules:/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/tty-table/adapters/node_modules:/Users/eric/Development/code/sports-betting/SPORTS_SCORES_API/node_modules/tty-table/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tty-table/adapters/terminal-adapter.js" "$@"
else
  exec node  "$basedir/../tty-table/adapters/terminal-adapter.js" "$@"
fi
